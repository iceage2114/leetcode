def removeDuplicates(nums):
    idx = 0
    count = 1

    for i in range(1, len(nums)):

        if nums[i] > nums[idx]:
            idx += 1
            count = 1
            nums[idx] = nums[i]

        elif nums[i] == nums[idx]:
            if count == 1:
                count += 1
                idx += 1
            elif count == 2:
                continue

        elif nums[i] < nums[idx]:
            idx += 1

        nums[idx] = nums[i]

    return idx + 1

nums = [0,0,1,1,1,1,2,3,3]
h = removeDuplicates(nums)
print(nums)
print(h)
# Output: 7, nums = [0,0,1,1,2,3,3,_,_]

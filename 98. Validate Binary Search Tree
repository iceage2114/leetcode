def isValidBST(root):
    def inorder(node):
        if not node:
            return []
        result = inorder(node.left) + [node.val] + inorder(node.right)
        return result

    inorder = inorder(root)

    for i in range(1, len(inorder)):
        if inorder[i - 1] >= inorder[i]:
            return False

    return True

def kidsWithCandies(candies, extraCandies):
    toReturn = max(candies)
    
    result = []
    
    for candy in candies:
        if candy + extraCandies >= toReturn:
            result.append(True)
        else:
            result.append(False)
    
    return result

candies = [2,3,5,1,3]
extraCandies = 3

print(kidsWithCandies(candies, extraCandies))
